{
  "name": "art-wallpaper",
  "version": "0.1.0",
  "private": true,
  "description": "An app that changes your wallpaper to random artworks from museums and plays music from MusicBrainz.",
  "keywords": [
    "art",
    "wallpaper",
    "music",
    "react",
    "three.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MariosPliatsikas/art-wallpaper-app.git"
  },
  "bugs": {
    "url": "https://github.com/MariosPliatsikas/art-wallpaper-app/issues"
  },
  "author": "Marios Pliatsikas <marios.pliatsikas@example.com>",
  "license": "MIT",
  "dependencies": {
    "eslint-config-react-app": "^7.0.1",
    "openseadragon": "^5.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write src/**/*.{js,jsx,css,json}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.0.0"
